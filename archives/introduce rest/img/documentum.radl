    <!-- AUTHENTICATION -->
    <authentication>
      <mechanism authentication-type="HTTP Basic" name="basic">

        <scheme name="basic">
            <documentation>HTTP basic authentication for Content Server inline users and LDAP users synced to Content
                Server.
            </documentation>
        </scheme>
        <scheme name="basic-ct">
            <documentation>HTTP basic authentication for Content Server inline users with Documentum Client Token cookie
                support.
            </documentation>
        </scheme>

      </mechanism>
      <mechanism authentication-type="HTTP Negotiate" name="kerberos">

        <scheme name="kerberos">
            <documentation>SPNEGO-based Kerberos authentication for Active Directory domain users synced to Content
                Server.
            </documentation>
        </scheme>
        <scheme name="ct-kerberos">
            <documentation>SPNEGO-based Kerberos authentication for Active Directory domain users synced to Content
                Server with Documentum Client Token cookie support.
            </documentation>
        </scheme>

      </mechanism>
      <mechanism authentication-type="CAS Web SSO" name="cas">

        <scheme name="cas-ct">
            <documentation>CAS SSO authentication with Documentum Client Token cookie support.</documentation>
        </scheme>

      </mechanism>
      <mechanism authentication-type="RSA ClearTrust SSO" name="rsa">

        <scheme name="rsa">
            <documentation>RSA ClearTrust SSO authentication with Documentum Client Token cookie support.
            </documentation>
        </scheme>

      </mechanism>
      <mechanism authentication-type="CA SiteMinder SSO" name="siteminder">

        <scheme name="siteminder">
            <documentation>CA SiteMinder SSO authentication with Documentum Client Token cookie support.</documentation>
        </scheme>

      </mechanism>
      <identity-provider mechanism="basic">
        <documentation>Documentum repository as the identity provider.</documentation>
      </identity-provider>
      <identity-provider mechanism="kerberos">
        <documentation>Kerberos domain controller as the identity provider.</documentation>
      </identity-provider>
      <identity-provider mechanism="cas">
        <documentation>Authentication provider behind the CAS server as the identity provider.</documentation>
      </identity-provider>
      <identity-provider mechanism="rsa">
        <documentation>RSA access manager as the identity provider.</documentation>
      </identity-provider>
      <identity-provider mechanism="siteminder">
        <documentation>SiteMinder access manager as the identity provider.</documentation>
      </identity-provider>
   </authentication>
</service>
