<service xmlns="urn:radl:service"
         xmlns:xi="http://www.w3.org/2001/XInclude"
         xmlns:html="http://www.w3.org/1999/xhtml"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         name="Documentum REST Services">
    <documentation>API References for Documentum REST Services</documentation>

    <!-- STATES -->
    <states>
      <start-state>
        <transitions>
            <transition name="Entry point" to="home-doc"/>
        </transitions>
      </start-state>
      <state name="home-doc">
        <transitions>
            <transition name="Get product information" to="product-info"/>
            <transition name="Get repositories" to="repositories"/>
        </transitions>
      </state>
      <state name="repositories">
        <transitions>
            <transition name="Find repository" to="repository"/>
        </transitions>
      </state>
      <state name="product-info">
        <transitions>
        </transitions>
      </state>
      <state name="repository">
        <transitions>
            <transition name="Get cabinets" to="cabinets"/>
            <transition name="Get checked out objects" to="checked-out-objects"/>
            <transition name="Create batch" to="batch">
                <input>
                    <properties name="batch-request"/>
                </input>
            </transition>
            <transition name="Get batch capabilities" to="batch-capabilities"/>
            <transition name="Execute DQL query" to="dql-query">
                <input>
                    <properties name="dql-query-request"/>
                </input>
            </transition>
            <transition name="Get users" to="users"/>
            <transition name="Get groups" to="groups"/>
            <transition name="Get current user" to="current-user"/>
            <transition name="Get formats" to="formats"/>
            <transition name="Get relation types" to="relation-types"/>
            <transition name="Get relations" to="relations"/>
            <transition name="Get network locations" to="network-locations"/>
            <transition name="Get types" to="types"/>
            <transition name="Execute full-text search" to="search"/>
        </transitions>
      </state>
      <state name="cabinets">
        <transitions>
            <transition name="Find cabinet" to="cabinet"/>
            <transition name="Create cabinet" to="cabinet">
              <input>
                  <properties name="cabinet">
                      <documentation>Mandatory properties for the cabinet creation.</documentation>
                      <property name="object_name"/>
                  </properties>
              </input>
            </transition>
        </transitions>
      </state>
      <state name="cabinet">
        <transitions>
            <transition name="Modify cabinet" to="cabinet">
                <input>
                    <properties name="cabinet"/>
                </input>
            </transition>
            <transition name="Remove cabinet"/>
            <transition name="Get child folders" to="folder-child-folders"/>
            <transition name="Get child documents" to="folder-child-documents"/>
            <transition name="Get child objects" to="folder-child-objects"/>
            <transition name="Get child links" to="child-folder-links"/>
        </transitions>
      </state>
      <state name="folder-child-folders">
        <transitions>
            <transition name="Find folder" to="folder"/>
            <transition name="Create folder" to="folder">
                <input>
                    <properties name="folder">
                        <documentation>Mandatory properties for the folder creation.</documentation>
                        <property name="object_name"/>
                    </properties>
                </input>
            </transition>
        </transitions>
      </state>
      <state name="folder">
        <transitions>
            <transition name="Modify folder" to="folder">
                <input>
                    <properties name="folder"/>
                </input>
            </transition>
            <transition name="Remove folder"/>
            <transition name="Get child folders" to="folder-child-folders"/>
            <transition name="Get child documents" to="folder-child-documents"/>
            <transition name="Get child objects" to="folder-child-objects"/>
            <transition name="Get child links" to="child-folder-links"/>
            <transition name="Get cabinet" to="cabinet"/>
            <transition name="Get canonical cabinet" to="cabinet"/>
            <transition name="Get parent folder" to="folder"/>
            <transition name="Get parent links" to="parent-folder-links"/>
        </transitions>
      </state>
      <state name="folder-child-documents">
        <transitions>
            <transition name="Find document" to="document"/>
            <transition name="Create document" to="document">
                <input>
                    <properties name="document"/>
                </input>
            </transition>
            <transition name="Create contentful document" to="document">
                <input>
                    <properties name="create-contentful-document-request"/>
                </input>
            </transition>
        </transitions>
      </state>
      <state name="document">
        <transitions>
            <transition name="Modify document" to="document">
                <input>
                    <properties name="document"/>
                </input>
            </transition>
            <transition name="Remove document"/>
            <transition name="Get contents" to="contents"/>
            <transition name="Get primary content" to="content"/>
            <transition name="Get all versions" to="all-versions"/>
            <transition name="Get current version" to="current-version"/>
            <transition name="Get precedent version" to="document"/>
            <transition name="Checkout" to="checked-out-object"/>
            <transition name="Get cabinet" to="cabinet"/>
            <transition name="Get parent folder" to="folder"/>
            <transition name="Get parent links" to="parent-folder-links"/>

            <transition name="Create comment" to="comment"/>
            <transition name="Get comments" to="comments"/>
        </transitions>
      </state>

      <state name="comments">
        <transitions>
          <transition name="Create comment" to="comment"/>
          <transition name="comment" to="comment"/>
        </transitions>
      </state>

      <state name="comment">
        <transitions>
          <transition name="Edit comment" to="comment"/>
          <transition name="Delete comment"/>
        </transitions>
      </state>

      <state name="folder-child-objects">
        <transitions>
            <transition name="Find sysobject" to="object"/>
            <transition name="Create sysobject" to="object">
                <input>
                    <properties name="object"/>
                </input>
            </transition>
            <transition name="Create contentful object" to="object">
                <input>
                    <properties name="create-contentful-document-request"/>
                </input>
            </transition>
            <transition name="Copy sysobject" to="object">
                <input>
                    <properties name="object-copy-ref"/>
                </input>
            </transition>
        </transitions>
      </state>
      <state name="object">
        <transitions>
            <transition name="Modify sysobject" to="object">
                <input>
                    <properties name="object"/>
                </input>
            </transition>
            <transition name="Remove sysobject"/>
            <transition name="Get contents" to="contents"/>
            <transition name="Get primary content" to="content"/>
            <transition name="Get all versions" to="all-versions"/>
            <transition name="Get current version" to="current-version"/>
            <transition name="Get precedent version" to="object"/>
            <transition name="Checkout" to="checked-out-object"/>
            <transition name="Get cabinet" to="cabinet"/>
            <transition name="Get parent folder" to="folder"/>
            <transition name="Get parent links" to="parent-folder-links"/>
            <transition name="Get canonical document" to="document"/>
            <transition name="Get canonical folder" to="folder"/>
            <transition name="Get canonical cabinet" to="cabinet"/>
        </transitions>
      </state>
      <state name="checked-out-objects">
        <transitions>
            <transition name="Find checked out object"/>
        </transitions>
      </state>
      <state extends="object" name="checked-out-object">
        <transitions>
            <transition name="Cancel checkout"/>
            <transition name="Check in as next major" to="object">
                <input>
                    <properties name="object"/>
                </input>
            </transition>
            <transition name="Check in as next minor" to="object">
                <input>
                    <properties name="object"/>
                </input>
            </transition>
            <transition name="Check in as branch" to="object">
                <input>
                    <properties name="object"/>
                </input>
            </transition>
            <transition name="Check in with content as next major" to="object">
                <input>
                    <properties name="create-contentful-object-request"/>
                </input>
            </transition>
            <transition name="Check in with content as next minor" to="object">
                <input>
                    <properties name="create-contentful-object-request"/>
                </input>
            </transition>
            <transition name="Check in with content as branch" to="object">
                <input>
                    <properties name="create-contentful-object-request"/>
                </input>
            </transition>
        </transitions>
      </state>
      <state name="child-folder-links">
        <transitions>
            <transition name="Find child folder link" to="child-folder-link"/>
            <transition name="Link to this folder" to="child-folder-link">
                <input>
                    <properties name="object-ref"/>
                </input>
            </transition>
        </transitions>
      </state>
      <state name="parent-folder-links">
        <transitions>
            <transition name="Find parent folder link" to="parent-folder-link"/>
            <transition name="Link to a specified folder" to="parent-folder-link">
                <input>
                    <properties name="object-ref"/>
                </input>
            </transition>
        </transitions>
      </state>
      <state name="child-folder-link">
        <transitions>
            <transition name="Remove child folder link"/>
        </transitions>
      </state>
      <state name="parent-folder-link">
        <transitions>
            <transition name="Remove parent folder link"/>
            <transition name="Move sysobject" to="parent-folder-link">
                <input>
                    <properties name="object-ref"/>
                </input>
            </transition>
        </transitions>
      </state>
      <state name="contents">
        <transitions>
            <transition name="Find content" to="content"/>
            <transition name="Add content" to="content">
                <input>
                    <properties name="content"/>
                </input>
            </transition>
        </transitions>
      </state>
      <state name="content">
        <transitions>
            <transition name="Remove content"/>
            <transition name="Get content media" to="content-media"/>
            <transition name="Get parent object" to="object"/>
        </transitions>
      </state>
      <state name="all-versions">
        <transitions>
            <transition name="Find revision" to="object"/>
        </transitions>
      </state>
      <state extends="object" name="current-version">
        <transitions>
            <transition name="Get canonical sysobject" to="object"/>
        </transitions>
      </state>
      <state name="content-media"/>
      <state name="batch"/>
      <state name="batch-capabilities"/>
      <state name="dql-query"/>
      <state name="groups">
        <transitions>
            <transition name="Find group" to="group"/>
        </transitions>
      </state>
      <state name="group">
        <transitions>
            <transition name="Get parent groups" to="groups"/>
            <transition name="Get sub groups" to="group-member-groups"/>
            <transition name="Get group users" to="group-member-users"/>
        </transitions>
      </state>
      <state name="group-member-groups">
        <transitions>
            <transition name="Find group" to="group"/>
        </transitions>
      </state>
      <state name="group-member-users">
        <transitions>
            <transition name="Find user" to="user"/>
        </transitions>
      </state>
      <state name="users">
        <transitions>
            <transition name="Find user" to="user"/>
        </transitions>
      </state>
      <state name="user">
        <transitions>
            <transition name="Get parent groups" to="groups"/>
            <transition name="Get default folder" to="default-folder"/>
        </transitions>
      </state>
      <state name="current-user">
        <transitions>
            <transition name="Get parent groups" to="groups"/>
            <transition name="Get default folder" to="default-folder"/>
        </transitions>
      </state>
      <state name="default-folder">
        <transitions>
            <transition name="Modify folder" to="folder"/>
            <transition name="Remove folder"/>
            <transition name="Get child folders" to="folder-child-folders"/>
            <transition name="Get child documents" to="folder-child-documents"/>
            <transition name="Get child objects" to="folder-child-objects"/>
            <transition name="Get child links" to="child-folder-links"/>
            <transition name="Get cabinet" to="cabinet"/>
            <transition name="Get canonical cabinet" to="cabinet"/>
            <transition name="Get parent folder" to="folder"/>
            <transition name="Get parent links" to="parent-folder-links"/>
        </transitions>
      </state>
      <state name="network-locations">
        <transitions>
            <transition name="Find network location" to="network-location"/>
        </transitions>
      </state>
      <state name="network-location"/>
      <state name="types">
        <transitions>
            <transition name="Find type" to="type"/>
        </transitions>
      </state>
      <state name="type">
        <transitions>
            <transition name="Get parent types" to="types"/>
            <transition name="Get parent type" to="type"/>
        </transitions>
      </state>
      <state name="formats">
        <transitions>
            <transition name="Find format" to="format"/>
        </transitions>
      </state>
      <state name="format"/>
      <state name="relation-types">
        <transitions>
            <transition name="Find relation type" to="relation-type"/>
        </transitions>
      </state>
      <state name="relation-type">
        <transitions>
            <transition name="Get relations" to="relations"/>
        </transitions>
      </state>
      <state name="relations">
        <transitions>
            <transition name="Find relation" to="relation"/>
            <transition name="Create relation" to="relation">
                <input>
                    <properties name="relation"/>
                </input>
            </transition>
        </transitions>
      </state>
      <state name="relation">
        <transitions>
            <transition name="Get relation type" to="relation-type"/>
            <transition name="Get parent object" to="object"/>
            <transition name="Get child object" to="object"/>
            <transition name="Remove relation"/>
        </transitions>
      </state>
      <state name="search">
        <transitions>
            <transition name="Find a search result"/>
            <transition name="Navigate a facet"/>
        </transitions>
      </state>
   </states>

    <!-- LINK RELATIONS -->
    <link-relations>
      <link-relation name="self">
        <documentation>Resource itself</documentation>
        <transitions>
            <!--<transition ref="Refresh"/>-->
            <transition ref="Remove content"/>
            <transition ref="Remove child folder link"/>
            <transition ref="Remove parent folder link"/>
            <transition ref="Move sysobject"/>
        </transitions>
      </link-relation>
      <link-relation name="edit on entry">
        <documentation>Get the entry by the 'edit' link relation on a specified entry of the feed</documentation>
        <transitions>
            <transition ref="Find repository"/>
            <transition ref="Find cabinet"/>
            <transition ref="Find folder"/>
            <transition ref="Find document"/>
            <transition ref="Find sysobject"/>
            <transition ref="Find content"/>
            <transition ref="Find revision"/>
            <transition ref="Find child folder link"/>
            <transition ref="Find parent folder link"/>
            <transition ref="Find checked out object"/>
            <transition ref="Find group"/>
            <transition ref="Find user"/>
            <transition ref="Find network location"/>
            <transition ref="Find type"/>
            <transition ref="Find format"/>
            <transition ref="Find relation type"/>
            <transition ref="Find relation"/>
            <transition ref="Find a search result"/>
        </transitions>
      </link-relation>
      <link-relation name="about">
        <documentation>Information about the product</documentation>
        <transitions>
            <transition ref="Get product information"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/repositories">
        <documentation>Repository list</documentation>
        <transitions>
            <transition ref="Get repositories"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/cabinets">
        <documentation>Cabinet list</documentation>
        <transitions>
            <transition ref="Get cabinets"/>
            <transition ref="Create cabinet"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/checked-out-objects">
        <documentation>Checked out objects list</documentation>
        <transitions>
            <transition ref="Get checked out objects"/>
        </transitions>
      </link-relation>
      <link-relation name="edit">
        <documentation>Modify the resource</documentation>
        <transitions>
            <transition ref="Modify cabinet"/>
            <transition ref="Modify folder"/>
            <transition ref="Modify document"/>
            <transition ref="Modify sysobject"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/delete">
        <documentation>Delete the resource</documentation>
        <transitions>
            <transition ref="Remove cabinet"/>
            <transition ref="Remove folder"/>
            <transition ref="Remove document"/>
            <transition ref="Remove sysobject"/>
            <transition ref="Remove relation"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/folders">
        <documentation>Folder child folders resource</documentation>
        <transitions>
            <transition ref="Get child folders"/>
            <transition ref="Create folder"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/documents">
        <documentation>Folder child folders resource</documentation>
        <transitions>
            <transition ref="Get child documents"/>
            <transition ref="Create document"/>
            <transition ref="Create contentful document"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/objects">
        <documentation>Folder child folders resource</documentation>
        <transitions>
            <transition ref="Get child objects"/>
            <transition ref="Create sysobject"/>
            <transition ref="Create contentful object"/>
            <transition ref="Copy sysobject"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/child-links">
        <documentation>Folder child folder links resource</documentation>
        <transitions>
            <transition ref="Get child links"/>
            <transition ref="Link to this folder"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/parent-links">
        <documentation>Folder parent folder links resource</documentation>
        <transitions>
            <transition ref="Get parent links"/>
            <transition ref="Link to a specified folder"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/cabinet">
        <documentation>Get cabinet resource</documentation>
        <transitions>
            <transition ref="Get cabinet"/>
        </transitions>
      </link-relation>
      <link-relation name="parent">
        <documentation>Get parent resource</documentation>
        <transitions>
            <transition ref="Get parent folder"/>
            <transition ref="Get parent object"/>
            <transition ref="Get parent type"/>
            <transition ref="Get parent groups"/>
        </transitions>
      </link-relation>
      <link-relation name="child">
        <documentation>Get child resource</documentation>
        <transitions>
            <transition ref="Get child object"/>
        </transitions>
      </link-relation>
      <link-relation name="canonical">
        <documentation>Get canonical resource</documentation>
        <transitions>
            <transition ref="Get canonical cabinet"/>
            <transition ref="Get canonical folder"/>
            <transition ref="Get canonical document"/>
            <transition ref="Get canonical sysobject"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/contents">
        <documentation>Contents of sysobject</documentation>
        <transitions>
            <transition ref="Get contents"/>
            <transition ref="Add content"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/primary-content">
        <documentation>Primary content of sysobject</documentation>
        <transitions>
            <transition ref="Get primary content"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/content-media">
        <documentation>Contents of sysobject</documentation>
        <transitions>
            <transition ref="Get content media"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/current-version">
        <documentation>Get current version</documentation>
        <transitions>
            <transition ref="Get current version"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/checkout">
        <documentation>Checkout</documentation>
        <transitions>
            <transition ref="Checkout"/>
        </transitions>
      </link-relation>
      <link-relation name="version-history">
        <documentation>Get all versions</documentation>
        <transitions>
            <transition ref="Get all versions"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/cancel-checkout">
        <documentation>Cancel checkout</documentation>
        <transitions>
            <transition ref="Cancel checkout"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/checkin-next-major">
        <documentation>Check in as next major</documentation>
        <transitions>
            <transition ref="Check in as next major"/>
            <transition ref="Check in with content as next major"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/checkin-next-minor">
        <documentation>Check in as next minor</documentation>
        <transitions>
            <transition ref="Check in as next minor"/>
            <transition ref="Check in with content as next minor"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/checkin-branch">
        <documentation>Check in as branch</documentation>
        <transitions>
            <transition ref="Check in as branch"/>
            <transition ref="Check in with content as branch"/>
        </transitions>
      </link-relation>
      <link-relation name="predecessor-version">
        <documentation>Get precedent version</documentation>
        <transitions>
            <transition ref="Get precedent version"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/batches">
        <documentation>Execute a batch request</documentation>
        <transitions>
            <transition ref="Create batch"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/batch-capabilities">
        <documentation>Batch capabilities</documentation>
        <transitions>
            <transition ref="Get batch capabilities"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/dql">
        <documentation>Execute a dql query</documentation>
        <transitions>
            <transition ref="Execute DQL query"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/groups">
        <documentation>Group list</documentation>
        <transitions>
            <transition ref="Get groups"/>
            <transition ref="Get sub groups"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/users">
        <documentation>User list</documentation>
        <transitions>
            <transition ref="Get users"/>
            <transition ref="Get group users"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/current-user">
        <documentation>Current user</documentation>
        <transitions>
            <transition ref="Get current user"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/default-folder">
        <documentation>User default folder</documentation>
        <transitions>
            <transition ref="Get default folder"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/network-locations">
        <documentation>Network location list</documentation>
        <transitions>
            <transition ref="Get network locations"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/types">
        <documentation>Type list</documentation>
        <transitions>
            <transition ref="Get types"/>
            <transition ref="Get parent types"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/relation-types">
        <documentation>Relation type list</documentation>
        <transitions>
            <transition ref="Get relation types"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/relation-type">
        <documentation>Get the relation type</documentation>
        <transitions>
            <transition ref="Get relation type"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/formats">
        <documentation>Format list</documentation>
        <transitions>
            <transition ref="Get formats"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/relations">
        <documentation>Relation list</documentation>
        <transitions>
            <transition ref="Get relations"/>
            <transition ref="Create relation"/>
        </transitions>
      </link-relation>
      <link-relation name="http://identifiers.emc.com/linkrel/search">
        <documentation>Execute full-text search</documentation>
        <transitions>
            <transition ref="Execute full-text search"/>
            <transition ref="Navigate a facet"/>
        </transitions>
      </link-relation>

      <link-relation name="http://identifiers.emc.com/linkrel/comments">
        <transitions>
          <transition ref="comment" to="comment"/>
          <transition ref="Create comment"/>
        </transitions>
      </link-relation>

      <link-relation name="http://identifiers.emc.com/linkrel/comment">
        <transitions>
          <transition ref="Edit comment" to="comment"/>
          <transition ref="Delete comment"/>
        </transitions>
      </link-relation>

   </link-relations>

    <!-- MEDIA TYPES -->
    <media-types>
      <media-type name="application/home+json">
        <documentation>JSON home document</documentation>
        <representation name="home-doc">
            <examples>
                <example>
{
  "resources": {
    "http://identifiers.emc.com/linkrel/repositories": {
      "href": "http://localhost:8080/dctm-rest/repositories",
      "hints": {
        "allow": [ "GET" ],
